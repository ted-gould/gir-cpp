#!/usr/bin/python3

import sys
from gi.repository import UbuntuAppLaunch as importedgir
import gi

def functionParse (func):
	return ''

def objectParse (obj):
	cppout = ''

	for item in obj.__dir__():
		if item.startswith('_'):
			continue
		if obj.__getattr__(item).__class__ is 'gi.FunctionInfo':
			cppout += functionParse(item)

	return cppout

if __name__ == '__main__':
	cpp = 'namespace GirCpp::%s {\n' % (importedgir._namespace)
	cpp += objectParse(importedgir);
	cpp += '}\n'
	print(cpp);
